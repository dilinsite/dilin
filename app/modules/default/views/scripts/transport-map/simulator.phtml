<script type="text/javascript">

function getFlashMovieObject(movieName){
	return document.getElementById(movieName);		    	
}


function SendDataToFlashMovie(str){
	flashMovie=getFlashMovieObject("map-flash");
        flashMovie.sendTextToFlash(str); 
}

                
$(document).ready(function(){ 
	
        $('#bSubmit').click(function() { 
            $('#transport_form_ajax_laoder').show();
            var id_leaf_origin = $('#transport-form .depart .sbSelector').attr('value');
            var id_leaf_destination = $('#transport-form .arrivee .sbSelector').attr('value');

            SendDataToFlashMovie(id_leaf_origin+'-'+id_leaf_destination);

            if (id_leaf_origin && id_leaf_destination) {
                getRelationMetaData(id_leaf_origin, id_leaf_destination, $('#transport-form .depart .sbSelector').html(), $('#transport-form .arrivee .sbSelector').html());
            }
        });

});



function getRelationMetaData(id_leaf_origin, id_leaf_destination, id_leaf_origin_name, id_leaf_destination_name) {
    $.ajax({
        type: "post",
        dataType: "json",
        url: '/transport-plan/get-relation-meta-data/id_leaf_origin/' + id_leaf_origin + '/id_leaf_destination/' + id_leaf_destination,
        success: function(res) {
                $('#transport_form_ajax_laoder').hide();
                $('#OutwardTripSchedule .label').html(id_leaf_origin_name + ' > ' + id_leaf_destination_name);
                $('#WaybackTripSchedule .label').html(id_leaf_destination_name + ' > ' + id_leaf_origin_name);
                $.each(res, function(k, v) {
                    $.each(v, function(k1, v1) {
                        if (k1 === 'ClosingBookingOrigin') {
                            $('#' + k1).html('Depart ' + id_leaf_origin_name + ': Jour ' + v1);
                        }
                        if (k1 === 'ClosingBookingDestination') {
                            $('#' + k1).html('Depart ' + id_leaf_destination_name + ': Jour ' + v1);
                        }
                        if (k1 === 'ClosingDocumentationOrigin') {
                            $('#' + k1).html('Depart ' + id_leaf_origin_name + ': Jour ' + v1);
                        }
                        if (k1 === 'ClosingDocumentationDestination') {
                            $('#' + k1).html('Depart ' + id_leaf_destination_name + ': Jour ' + v1);
                        }
                        if (k1 === 'MADOrigin') {
                            $('#' + k1).html(v1);
                        }
                        if (k1 === 'ClosingOrigin') {
                            <?php if (isset($_COOKIE['locale']) && $_COOKIE['locale'] == 'en_US') { ?>
                                $('#' + k1).html(v1.wording_uk_en);
                            <?php } else { ?>
                                $('#' + k1).html(v1.wording_fr_fr);
                            <?php } ?>
                        }
                        if (k1 === 'EnvironmentalInformation') {
                            var digits = v1.co2_saving.toString().split('');
                            var content = '';
                            for (var i = 0; i < digits.length; i++) {
                                content += '<span class="big_number">' + digits[i] + '</span>';
                            }
                            $('#' + k1).html(content);
                        }
                        if (k1 === 'OutwardTripSchedule' || k1 === 'WaybackTripSchedule') {
                                getTripSchedule(k1, v1);
                        }
                    });
                });
                $('.transport-form-result').show();
        }
    });
}

function getTripSchedule(k1, v1) {
    var content = '';
    <?php if (isset($_COOKIE['locale']) && $_COOKIE['locale'] == 'en_US') { ?>
    $.each(v1, function(k2, v2) {
        var tr = '<tr>';
        $.each(v2,function(k3, v3) {
            if (k3 === 'HRL' || k3 === 'MAD')
                tr += '<td>' + getEnDay(v3.day) + ' ' + v3.hour + 'h'+v3.minute + '</td>';
            else if (k3 === 'delay')
                tr += '<td>' + v3 + '</td>';
        });
        tr+='</tr>';
        content+=tr;
    });
    <?php } else { ?>
    $.each(v1, function(k2, v2) {
        var tr = '<tr>';
        $.each(v2,function(k3, v3) {
            if (k3 === 'HRL' || k3 === 'MAD')
                tr += '<td>' + getFrDay(v3.day) + ' ' + v3.hour + 'h'+v3.minute + '</td>';
            else if (k3 === 'delay')
                tr += '<td>' + v3 + '</td>';
        });
        tr+='</tr>';
        content+=tr;
    });
    <?php } ?>
    $('#' + k1 + ' table').html('<tr><th>HLR<span>*</span></th><th>MAD<span>*</span></th><th>Delai</th></tr>' + content);
}
</script>